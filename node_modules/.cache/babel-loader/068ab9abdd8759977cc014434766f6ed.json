px\">\n  <el-form-item label=\"截止时间\">\n    <el-col :span=\"15\">\n      \n        <el-date-picker type=\"datetime\" placeholder=\"截止时间\" v-model=\"datetime\" value-format=\"yyyy-MM-dd HH:mm:ss\" format=\"yyyy-MM-dd HH:mm:ss\" style=\"width: 100%;\"></el-date-picker>\n        \n        \n    </el-col>\n    \n  </el-form-item>\n  <el-form-item label=\"事项标题\">\n    <el-input v-model=\"title\"></el-input>\n  </el-form-item>\n<el-form-item label=\"事项内容\">\n    <el-input v-model=\"content\"></el-input>\n  </el-form-item>\n  <el-form-item>\n    <el-button type=\"primary\" @click=\"add()\">立即创建</el-button>\n    <el-button @click=\"jump()\">取消</el-button>\n  </el-form-item>\n</el-form>\n\n\n</div>\n\n</template>\n\n<script>\nimport store from '@/store'\nexport default {\n  name: \"add\",\n  store,\n  data () {\n    return {\n      title:'',\n      content:'',\n      datetime:''\n    };\n  },\n\n  components: {},\n\n  computed: {\n  },\n\n  mounted: {},\n\n  methods: {\n    jump(){\n      this.$router.push('/home/list')\n    },\n    add(){\n      if (this.title==''){\n        window.alert(\"标题不可为空\");\n        return ;\n      }\n      store.commit('addItem',{\n        title:this.title,\n        content:this.content,\n        datetime:this.datetime\n      })\n      this.title='',\n      this.content='',\n      this.$router.push('/home/list')\n    }\n  }\n}\n\n</script>\n<style lang='scss' scoped>\n</style>\n\n"],"sourceRoot":"src/views"}]}